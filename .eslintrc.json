{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react", "jsx-a11y", "import", "react-native"],
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/forbid-prop-types": [
      1,
      {
        "forbid": ["any"]
      }
    ],
    "linebreak-style": 0,
    "import/extensions": 0,
    "import/no-unresolved": 0
  },
  "env": {
    "jest": true
  }
}
